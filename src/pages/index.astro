---
import DefaultLayout from "../layouts/DefaultLayout.astro"
import { getPage } from "../lib/hygraph.js"

interface Page {
  title: string
  content: string
}

const page = (await getPage("start")) as Page
---

<DefaultLayout title="Start">
  <h1>{page.title}</h1>
  <div>{page.content}</div>
</DefaultLayout>
