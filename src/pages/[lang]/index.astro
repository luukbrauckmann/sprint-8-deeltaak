---
import Page from "../../layouts/Page.astro"
import { getPage } from "../../lib/hygraph"

const { lang } = Astro.params

interface Page {
  title: string
  content: string
}

const page = (await getPage(lang, "start")) as Page
---

<Page title="Start">
  <h1>{page.title}</h1>
  <div>{page.content}</div>
</Page>
